{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "job-interview-todolist-fe-docs",
  "version": "0.0.0",
  "description": "Modern todo list with React 19, Redux Toolkit, and TypeScript",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --run --ui",
    "typecheck": "tsc --noEmit --project ./tsconfig.app.json"
  },
  "dependencies": {
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@reduxjs/toolkit": "^2.9.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "effect": "^3.18.4",
    "immer": "^10.2.0",
    "lucide-react": "^0.548.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.3",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "npm:rolldown-vite@^7.1.19",
    "vitest": "^4.0.3"
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.{json,yml,yaml}": "prettier --write"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.19.0",
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@^7.1.19"
    }
  }
}
